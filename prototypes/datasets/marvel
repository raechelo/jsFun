// create an obj with each characters name as a key and their powers as the value

// example:'Steven Rogers': 
  //  [ 'Superhuman Agility',
  //    'Superhuman Stamina',
  //    'Superhamn Durability',
  //    'Superhuman Strength',
  //    'Superhuman Reflexes' ]

const result = marvel.reduce((acc, char) => {
  if (!acc[char.name]) {
    acc[char.name] = []
  }
  char.powers.forEach(power => acc[char.name].push(power))
  return acc
}, {})



// create a object with the teams as the keys and the alias of the characters belonging to those groups as the values
// example: 
// 'The Avengers': [ 'Captain America', 'Iron Man', 'Black Widow' ]

const teams = marvel.reduce((acc, char) => {
  char.teams.forEach(team => {
    if (!acc[team]) {
      acc[team] = [] 
    }
    acc[team].push(char.alias)
  })
  return acc
}, {})



// create an obj with the keys of debutYear and the values the alias
// example: 1968: [ 'Captain Marvel' ],
     // 1974: [ 'Wolverine' ]

const debut = marvel.reduce((acc, char) => {
  if (!acc[char.debutYear]) {
    acc[char.debutYear] = []
  }
  acc[char.debutYear].push(char.alias)
  return acc
}, {})



//return an array of objects of only the Omega Level Mutants
// example:
// [ 'Charles Xavier', 'Jean Grey', 'Wanda Maximoff' ]
let omega = marvel.reduce((acc, hero) => {
  hero.powers.forEach(power => {
    if (power === 'Omega Level' && !acc.includes(hero.name)) {
      acc.push(hero.name)
    }
  })
  return acc
}, [])

console.log(omega)
